<body id="admin">
	
	<a href="${webbaseurl}/manageCourseFull.action">返回管理</a>
<section id="main-wrap" class="clearfix"><!--main-wrap-->

      <!--admin-wrap start-->
      <div id="admin-wrap">
        <div id="admin-content" class="clearfix">    
            <!--control-bar start-->
            <div class="control-bar clearfix">
             

             
	  
	  
 <div class="panel-body">
                 <!-- form start -->
               <form action="" method="" name="AddCourseForm">
				 <input type="hidden" class="form-control" id="input_course_id" value="$!courseFull.courseid">


		<div class="form-group">
                    <label for="name" class="col-lg-2 control-label">name</label>
                    <div class="col-lg-4">
                      <input type="text" class="form-control" id="input_course_name" value="$!courseFull.coursename">
                    </div>
                  </div>

		<div class="form-group">
                    <label for="description" class="col-lg-2 control-label">description</label>
                    <div class="col-lg-4">
                      <input type="text" class="form-control" id="input_course_description" value="$!courseFull.coursedescription">
                    </div>
                  </div>

		<div class="form-group">
                    <label for="type" class="col-lg-2 control-label">type</label>
                    <div class="col-lg-4">
                      <input type="text" class="form-control" id="input_course_type" value="$!courseFull.coursetype">
                    </div>
                  </div>

		<div class="form-group">
                    <label for="adduserid" class="col-lg-2 control-label">adduserid</label>
                    <div class="col-lg-4">
                      <input type="text" class="form-control" id="input_course_adduserid" value="$!courseFull.courseadduserid">
                    </div>
                  </div>

		<div class="form-group">
                    <label for="teacherid" class="col-lg-2 control-label">teacherid</label>
                    <div class="col-lg-4">
                      <input type="text" class="form-control" id="input_course_teacherid" value="$!courseFull.courseteacherid">
                    </div>
                  </div>

		<div class="form-group">
                    <label for="addtime" class="col-lg-2 control-label">addtime</label>
                    <div class="col-lg-4">
                      <input type="text" class="form-control" id="input_course_addtime" value="$!courseFull.courseaddtime">
                    </div>
                  </div>

               </form>
               <!-- form end -->
			  <!-- submit start -->
			   <div class="form-group">   
                  <div class="col-lg-offset-2 col-lg-4">
                     <button class="btn btn-white" data-dismiss="modal" aria-hidden="true">取 消</button>
                     <button type="submit" onClick = "updateCourseAjax()" class="btn btn-primary">修改</button>
                   </div>
               </div>
				<!-- submit end -->




              </div>
            </div>
          </div>

      </div>
      <!--equip-wrap end-->
    
      <!--sidebar end-->

</section>



