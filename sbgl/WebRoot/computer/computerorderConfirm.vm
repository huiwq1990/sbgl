<script>
var cookie = { 
  		//读取COOKIES,n为COOKIE名 
   		Get:function(n){ 
       		var re=new RegExp(n+'=([^;]*);?','gi');
       	    var r=re.exec(document.cookie)||[];
       	    return (r.length>1?r[1]:null)
   		}
	};	
	

//提交预约单
	$("body").on("click", "button[action-type|=orderComputerButton]", function() {
	
		var createuserid = cookie.Get("id");
		var postData = {
			title : $("#inputAddComputerorderTitle").val(),
			remark : $("#inputAddComputerorderRemark").val(),
			ordertype : $computerordertype
		};
		
		jQuery.ajax({  
		    url:"${webbaseurl}/addComputerorderAjax.${strutsaction}",  
		    type:"post",
		    data:postData,
			dataType: 'json',
			success: function(data){
				var returnJson = jQuery.parseJSON(data); 
				if(returnJson.flag == 1){					
					location.href = "${webbaseurl}/toComputerorderSuccessPage.${strutsaction}";
				}else{
					var n = noty({text: returnJson.reason, timeout: 2500, type: 'success'});					
					$(this).parents('.modal').modal('toggle');		// 提交成功，关闭模态对话框		
				}
			}		  	  
		});//end jquery
	});
//end Order


</script>
