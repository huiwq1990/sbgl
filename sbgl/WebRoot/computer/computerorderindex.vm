<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8">
<title>北京电影学院摄影系 - 预约</title>
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="description" content="">
<meta name="author" content="">
<meta name="viewport" content="width=device-width, initial-scale=1.0">

<link href="${cssDomain}/bootstrap.css" rel="stylesheet" type="text/css" />
<link href="${cssDomain}/global.css" rel="stylesheet" type="text/css" />

#parse("include/includePageCompatibility.vm")

</head>

<body>
<header id="header-wrap"><!--header-wrap-->

	#parse("include/includeGlobalHeader.vm")
	
  <div class="nav-wrap clearfix">
	#parse("include/includeSiteNavi.vm")	
		
	#set($currentPageType="computerorderSpace")
	#parse("include/includeOrderspaceSubNavi.vm")
  </div>

</header><!--/#header-wrap-->
<section id="main-wrap"><!--main-wrap-->
  <!--main-content start-->
  <div id="main-content" class="clearfix">
      <!--equip-wrap start-->
   
          
          <div class="col3 container">
            <div class="control-bar clearfix">
              <div class="title pull-left">进行中的预约</div>
              <a class="btn btn-primary pull-right" href="${webbaseurl}/toComputerIndividualorderPage.${strutsaction}" role="button" data-toggle="modal">创建预约</a>
            </div>
            <div class="row">
      
#set($computerorderFullListSize = $computerorderFullUnderwayList.size() - 1)
##判断数据是否为空
#if($computerorderFullListSize>=0)
#set($computerorderFullListRegion = [0..$computerorderFullListSize])
##取出所有的属性名称
#foreach($tempKey in $computerorderFullListRegion)
			#set($temp = $computerorderFullUnderwayList.get($tempKey))				
                  <div class="col-3">                  
                      <div class="thumbnail">
                        <div class="hd clearfix">
                          <h4 class="pull-left"><a class="lk" href="${webbaseurl}/viewComputerorder.action?computerorderId=$temp.computerorderid">$!temp.computerordertitle</a></h4>
                        </div>
                         <div class="t-r rent-type label label-important"><i class="icon-camera-retro icon-large"></i> 
							机房预约<br>
							#if($!temp.computerorderordertype==2) 
								课程预约 
							#else 
								个人预约 
							#end
						</div>
						<div class="caption">
                          <div class="rent-info">
                            <div class="rent-num">预约号：$!temp.computerorderserialnumber</div>
							
                            <div class="rent-time"><i class="icon-time"></i>$date.format("yyyy年MM月dd日",$!temp.computerordercreatetime)</div>
                            <div class="rent-remark"><i class="icon-pencil">$!temp.computerorderremark</i></div>
                          
						  </div>
                          <hr />
						  #if($!temp.computerorderstatus == $ComputerorderStatusAduitWait)
                          <div class="rent-status clearfix">
							#set($curstatus = $temp.computerorderstatus)
                            <div class="status pull-left"><span class="label label-info">状态</span>#parse("computer/includeComputerorderStatus.vm")</div>
                          </div>
						  #elseif($!temp.computerorderstatus == $ComputerorderStatusAduitReject)
							  <div class="rent-status clearfix">
                            <div class="status pull-left"><span class="label label-warning">状态</span> 预约被驳回，需修改后重新提交预约</div>
                            <a class="btn btn-white pull-right" action-type="" action-data="" href="${webbaseurl}/viewComputerorder.action?computerorderId=$temp.computerorderid">查看原因</a>
                          </div>
						  #else
							  <div class="rent-status clearfix">
                            <div class="status pull-left"><span class="label label-error">状态</span> 状态出错</div>
                          </div>
							#end
                        </div>
                      </div>                  
                  </div>
#end
#end				  
              
            </div>
            
            <hr class="hr-block" />
            <div class="control-bar clearfix">
              <div class="title pull-left">已完成</div>
            </div>
            <div class="row thumbnails">

#set($computerorderFullListSize = $computerorderFullFinishList.size() - 1)
##判断数据是否为空
#if($computerorderFullListSize>=0)
#set($computerorderFullListRegion = [0..$computerorderFullListSize])
##取出所有的属性名称
#foreach($tempKey in $computerorderFullListRegion)
			#set($temp = $computerorderFullFinishList.get($tempKey))				
                  <div class="col-3">                  
                      <div class="thumbnail">
                        <div class="hd clearfix">
                          <h4 class="pull-left"><a class="lk" href="${webbaseurl}/viewComputerorder.action?computerorderId=$temp.computerorderid">$!temp.computerordertitle</a></h4>
                        </div>
                        <div class="t-r rent-type label label-important"><i class="icon-camera-retro icon-large"></i> 
							机房预约<br>
							#if($!temp.computerorderordertype==2) 
								课程预约 
							#else 
								个人预约 
							#end
						</div>
                        <div class="caption">
                          <div class="rent-info">
                            
							   <div class="rent-num">预约号：$!temp.computerorderserialnumber</div>							
                            <div class="rent-time"><i class="icon-time"></i>$date.format("yyyy年MM月dd日",$!temp.computerordercreatetime)</div>
                            <div class="rent-remark"><i class="icon-pencil">$!temp.computerorderremark</i></div>
                          
						  
						  </div>
                          <hr />
                          <div class="rent-status clearfix">
							#set($curstatus = $temp.computerorderstatus)
                            <div class="status pull-left"><span class="label label-success">状态</span> #parse("computer/includeComputerorderStatus.vm")</div>
                          </div>
                        </div>
                      </div>                  
                  </div>
#end
#end				
				
				<!--
                  <div class="col-3">                  
                      <div class="thumbnail">
                        <div class="hd">
                          <h4><a class="lk" href="设备预约详细内容页.html">2013年10月18日-23日设备预约</a></h4>
                        </div>
                        <div class="t-r label label-important"><i class="icon-camera-retro icon-large"></i> 设备预约<br/>个人预约</div>
                        <div class="caption">
                          <div class="rent-info">
                            <div class="rent-num">预约号：432420427（无则不显示）</div>
                            <div class="rent-time"><i class="icon-time"></i> 2013年10月18日 - 2013年10月23日（无则不显示）</div>
                            <div class="rent-remark"><i class="icon-pencil"></i> 这里是预约备注（无则不显示）</div>
                          </div>
                        </div>
                      </div>                  
                  </div>
				  -->
            </div>            
            

          </div>
  </div>
  <!---main-content end-->
</section>


    <!-- Le javascript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
<script src="${jsDomain}/jquery.js"></script>
<script src="${jsDomain}/loader.min.js"></script>
<script src="${jsDomain}/select2.js"></script>
<script src="${jsDomain}/bootstrap-hover-dropdown.min.js"></script>
<script src="${jsDomain}/bfa.js"></script>


<!-- Modal -->
<div id="selectRentType" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="selectRentTypeLabel" aria-hidden="true">
  <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true"><i class="icon-remove"></i></button>
    <h3 id="myModalLabel">选择</h3>
  </div>
  <div class="modal-body">
    <div class="text-center clearfix">
      <a class="btn btn-white btn-large btn-type pull-left" href="设备预约页.html"><h4><i class="icon-camera-retro icon-2x"></i></h4><h3>设备预约</h3></a>
      <a class="btn btn-white btn-large btn-type pull-right" href="机房预约页.html"><h4><i class="icon-desktop icon-2x"></i></h4><h3>机房预约</h3></a>
    </div>
  </div>
  <div class="modal-footer">
    <button class="btn btn-white" data-dismiss="modal" aria-hidden="true">取 消</button>
  </div>
</div>

<div id="popover_user_wrapper" style="display: none" onmouseover="$(this).mouseleave(function() {$(this).hide(); });">
  <img src="img/cover-img.jpg" class="cover" alt="封面照片" />
  <div class="cover-layout"></div>
  <div class="avatar"><img src="img/photo.jpg" width="80" height="80" class="img-circle"></div>
  <div class="user-wrap">
    <div class="user-name"><a href="#">用户名</a></div>
    <div class="user-info" title="这里是一句话简介,这里是一句话简介,这里是一句话简介,这里是一句话简介">这里是一句话简介，这里是一句话简介，这里是一句话简介</div>
    <div class="user-info">职务等信息</div>
    <div class="user-info">地点</div>
  </div>
  <div class="contact-wrap"><a href="#" title="发消息"><i class="icon-envelope"></i></a></div>
</div>

</body>
</html>