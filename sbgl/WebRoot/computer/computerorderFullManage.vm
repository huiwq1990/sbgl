<script>
var cookie = { 
  		//读取COOKIES,n为COOKIE名 
   		Get:function(n){ 
       		var re=new RegExp(n+'=([^;]*);?','gi');
       	    var r=re.exec(document.cookie)||[];
       	    return (r.length>1?r[1]:null)
   		}
	};

//分页信息
var currentpagenum=1;
var totalpage=$page.totalpage;
var computerorderStatus = $computerorderStatus;
//换页时调用方法
$('.pagination').jqPagination({
    	paged: function(page) {
     	   // do something with the page variable
		   alert("next page");
			currentpagenum = page;
			refreshComputerorderTable();
    	}
});
//刷新换页后的信息
function refreshComputerorderTable(){
			jQuery.ajax({  
		    url: '${webbaseurl}/manageComputerorderFull.action',  
		    type: "post",
		    data:{	pageNo : currentpagenum,
					callType:'ajaxType',
					computerorderStatus : computerorderStatus
					},
		    dataType: 'html',
			success: function(data){
				//alert("refreshing");
				$("#computerordermanagetable").html(data);
				
				$('.pagination').jqPagination({
					link_string	: '/?page={page_number}',
					max_page	: totalpage,
					current_page:currentpagenum,
					paged: function(page) {     	  
						currentpagenum = page;
						computerorderStatus : computerorderStatus;
						refreshComputerorderTable();
		
    				}
				});
				}		  	  
			});		
}


function getComputerorderByAuditStatus(status){
	//alert("getComputerorderByAuditStatus");
	currentpagenum = 1;
	computerorderStatus = status;
	
	refreshComputerorderTable();
}



</script>
