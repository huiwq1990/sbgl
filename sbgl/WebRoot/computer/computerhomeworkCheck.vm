<script>

function auditOrder(id,status){
	jQuery.ajax({  
		url: '${webbaseurl}/auditComputerorderAjax.action',  
		type: "post",
		data:{id:id,status:status},
		dataType: 'json',
		success: function(data){
		
			var returnJson = jQuery.parseJSON(data); 
			
			if(returnJson.flag == 1){
				alert(returnJson.reason);
			}
		}		  	  
		});

}

</script>
