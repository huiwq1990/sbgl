<script>
	var cookie = { 
  		//读取COOKIES,n为COOKIE名 
   		Get:function(n){ 
       		var re=new RegExp(n+'=([^;]*);?','gi');
       	    var r=re.exec(document.cookie)||[];
       	    return (r.length>1?r[1]:null)
   		}
	};
	var editComputercategoryId;
	var editComputercategoryName;

//选择修改按钮，填充修改弹出框
	$("body").on("click", "a[action-type|=editComputerhomeworkreceiverFillIn]", function() {
		var data = $(this).data();
							$("#inputEditComputerhomeworkreceiverid").val(data.id);       
			var editid = data.id;
					$("#inputEditComputerhomeworkreceivercomputerhomeworkid").val(data.computerhomeworkid);       
			var editcomputerhomeworkid = data.computerhomeworkid;
					$("#inputEditComputerhomeworkreceiveruserid").val(data.userid);       
			var edituserid = data.userid;
			})
//end 	editComputerhomeworkreceiverFillIn

// 触发提交按钮事件
$('.modal').on('click', ('button.btn-primary'), function () {		
		var _this = $(this);

		//处理添加
		if(_this.attr('action-type') == 'addComputercategoryButton')
		{
			//alert('test add');
			var actionUrl = "${webbaseurl}/addComputercategoryAjax.action";
			var inputCategoryName = $('#addComputercategoryName').val();			
			var postData = {
				"createuserid": cookie.Get("id"),
				"name": inputCategoryName		
			}
			var successStr = "添加分类成功！";
			var failedStr = "添加分类失败！";				
		}
		// ebd add
		
		//处理update
		if(_this.attr('action-type') == 'updateComputerhomeworkreceiverButton')
		{
			//alert('test add');
			var actionUrl = "${webbaseurl}/updateComputerhomeworkreceiverAjax.action";
												var id = $("#inputEditComputerhomeworkreceiverid").val();      
								var computerhomeworkid = $("#inputEditComputerhomeworkreceivercomputerhomeworkid").val();      
								var userid = $("#inputEditComputerhomeworkreceiveruserid").val();      
						var postData = {
													id: id,
									computerhomeworkid: computerhomeworkid,
									userid: userid,
							}
		}
		//end update
		
		
		
		// 如果是其它的提交操作，对照上一行的if代码处理		
		
		// 提交处理
		jQuery.ajax({  
		    url: actionUrl,  
		    type: "post",
		    data: postData,
		    dataType: 'json',
			success: function(data){
				var returnJson = jQuery.parseJSON(data); 
				if(returnJson.flag == 1){
					var n = noty({text: returnJson.reason, timeout: 2500, type: 'success'});					
					_this.parents('.modal').modal('toggle');		// 提交成功，关闭模态对话框		
					
				}else{
					var n = noty({text: returnJson.reason, timeout: 2500, type: 'error'});
				}
			}		  	  
		});		
	})




	
	//更新分类
	$("body").on("click", "button[action-type|=updateComputercategoryButton]", function() {
		var modelName = $("#inputEditComputercategoryName").val();

			var postData = {
				"id": editComputercategoryId,
				"name": modelName
				
			}
			
			jQuery.ajax({  
			    url:"${webbaseurl}/updateComputercategoryAjax.${strutsaction}",  
			    type:"post",
			    data:postData,
			  	dataType: 'json',
				success: function(data){
				var returnJson = jQuery.parseJSON(data); 
			//alert(returnJson.flag);
			if(returnJson.flag==1){
				alert("保存成功!")
			}else{
				alert("保存失败!")
			}
		}	});		  	  

	})
	//end update
	
	
	
	var idsForDel;	
	/* 处理CheckBox选中事件用于删除处理 */
	$("input[type='checkbox']").click(function() {
		//var num = 0;
		idsForDel = "";
		
		if($(this).attr("id") === "chk-all") {
			$("input[name='chk-list']").prop("checked",$(this).prop("checked"));
		}
		$("input[name='chk-list']").each(function() {
			if($(this).prop("checked") === true) {
				//num++;
				idsForDel += $(this).prop("value") + ";";
			}
		});
	});
	//end box
	
	//delete分类
	$("body").on("click", "a[action-type|=deleteComputercategory]", function() {
		idsForDel = idsForDel.substring(0,idsForDel.length-1);
		var postData = {
			computercategoryIdsForDel: idsForDel
		}
		//alert(idsForDel);
		jQuery.ajax({  
		    url:"${webbaseurl}/deleteComputercategoryFullAjax.${strutsaction}",  
		    type:"post",
		    data:postData,
			  dataType: 'json',
			success: function(data){
				var returnJson = jQuery.parseJSON(data); 

				
				if(returnJson.flag == 1){
					var n = noty({text: 'sss', timeout: 2500, type: 'success'});					
					$(this).parents('.modal').modal('toggle');		// 提交成功，关闭模态对话框					
				}else{
					var n = noty({text: failedStr, timeout: 2500, type: 'error'});
				}

				
				
				}		  	  
			});
	})
	//end delete 分类
	
	
	
	//换页时调用方法
$('.pagination').jqPagination({
    	paged: function(page) {
     	   // do something with the page variable
			currentpagenum = page;
			refreshComputerorderclassruleTable();
    	}
	});
	
//刷新数据
function refreshComputerorderclassruleTable(){
			jQuery.ajax({  
		    url: '${webbaseurl}/manageComputerorderclassruleFull.action',  
		    type: "post",
		    data:{	
					pageNo:currentpagenum,
					callType:'ajaxType'					
					},
		    dataType: 'html',
			success: function(data){
				$("#refreshComputerTableDiv").html(data);
				
				//设置分页绑定事件
				$('.pagination').jqPagination({
					link_string	: '/?page={page_number}',
					max_page	: totalpage,
					current_page:currentpagenum,
					paged: function(page) {     	  
						currentpagenum = page;
						refreshComputerorderclassruleTable();		
    				}
				});
				}		  	  
			});		
}//end refreshComputerorderclassruleTable
	
</script>


