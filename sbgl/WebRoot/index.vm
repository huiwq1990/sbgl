<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8">
<title>北京电影学院摄影系</title>
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="description" content="">
<meta name="author" content="">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<link href="${cssDomain}/bootstrap.css" rel="stylesheet" type="text/css" />
<link href="${cssDomain}/global.css" rel="stylesheet" type="text/css" />


#parse("include/includePageCompatibility.vm")


</head>

<body>
<header id="header-wrap"><!--header-wrap-->

		#parse("include/includeGlobalHeader.vm")
  
  <div class="nav-wrap clearfix">	
	#parse("include/includeSiteNavi.vm")	
	##set($currentPageType="createorder")
	##parse("include/includeOrderspaceSubNavi.vm") 
  </div>
	
</header><!--/#header-wrap-->
<section id="user-header"><!-- user-header -->
<div class="container">
  <div class="user-content thumbnail">
    <div id="user-profile">
      <div class="media">
        <div id="user-profile-photo" class="pull-left">
          <img src="img/photo.jpg" width="120" height="120" class="img-circle">
        </div>
        <div id="user-profile-info" class="media-body">
          <h4 class="profile-name media-heading">Frank Chen 陈诚</h4>
          <div class="profile-more-info">2012级研究生 - 影像视觉效果研究方向</div>
        </div>
      </div>
    </div>
    <div class="user-bg-pic"><img src="img/img8.jpg" class="img-responsive"></div>
    <nav class="user-nav">
      <ul class="nav nav-tabs nav-tabs-google">
        <li><a href="${webbaseurl}/indexInfo.do">简介</a></li>
        <li class="active"><a href="index.html">信息</a></li>
      </ul>
    </nav>
  </div>
</div>  
</section><!-- /.user-header -->
<section id="main-wrap"><!--main-wrap-->

  <div id="main-content" class="clearfix"><!--main-content start-->
    <div id="stream-wrap">
      <div id="stream" class="container">
        <div class="row">
          <div class="col-md-6">
            <div class="thumbnail">
              <div class="hd clearfix">
                <h3 class="pull-left">新的</h3>
              </div>
              <div class="row p-20">
                <div class="col-md-4">
                  <a class="btn btn-white btn-block" href="设备预约页.html"><h4><i class="icon-camera-retro icon-2x"></i></h4><h4>设备预约</h4></a>
                </div>
                <div class="col-md-4">
                  <a class="btn btn-white btn-block" href="机房预约页.html"><h4><i class="icon-desktop icon-2x"></i></h4><h4>机房预约</h4></a>
                </div>
                <div class="col-md-4">
                  <a class="btn btn-white btn-block" href="消息中心-站内信-写站内信.html"><h4><i class="icon-envelope icon-2x"></i></h4><h4>站内信</h4></a>
                </div>
              </div>
            </div>
            <div class="thumbnail">
              <div class="hd clearfix">
                <h3 class="pull-left">站内信</h3>
                <a href="消息中心-站内信.html" class="more lk pull-right">查看全部</a>
              </div>
              <ul class="media-list">
                <li class="media">
                  <a rel="popover" class="pull-left" href="#">
                    <img src="img/photo.jpg" width="46" height="46" class="img-rounded">
                  </a>
                  <div class="media-body">
                    <div class="media-heading"><h5><a rel="popover" href="消息中心-站内信-查看站内信.html">李全胜</a></h5><span>7月6日</span></div>
                    <a href="#">回复：关于HDR图像的问题</a>
                  </div>
                </li>
                <li class="media">
                  <a rel="popover" class="pull-left" href="#">
                    <img src="img/photo.jpg" width="46" height="46" class="img-rounded">
                  </a>
                  <div class="media-body">
                    <div class="media-heading"><h5><a rel="popover" href="#">林韬</a></h5><span>7月6日</span></div>
                    <a href="#">第11周摄影作业要求</a>
                  </div>
                </li>
              </ul>
            </div>
          </div>
          <div class="col-md-6">
            <div class="thumbnail">
              <div class="hd clearfix">
                <h3 class="pull-left">进行中的设备预约</h3>
                <a href="预约首页.html" class="more lk pull-right">查看全部</a>
              </div>
              <ul class="media-list">
                <li class="media">
                  <div class="media-body">
                    <div class="media-heading"><h5><a href="设备预约详细内容页.html">数字摄影技术第11周设备申请通知</a></h5><span>等待你提交预约申请</span></div>
                    <span class="tag label label-important">课程预约</span>                  
                  </div>
                </li>
                <li class="media">
                  <div class="media-body">
                    <div class="media-heading"><h5><a href="设备预约详细内容页.html">2013年6月18日设备申请</a></h5><span>等待审核</span></div>
                    <span class="tag label label-important">个人预约</span>                  
                  </div>
                </li>
              </ul>
            </div>
            <div class="thumbnail">
              <div class="hd clearfix">
                <h3 class="pull-left">进行中的机房预约</h3>
                <a href="${webbaseurl}/viewMineComputerorderList.${strutsaction}" class="more lk pull-right">查看全部</a>
              </div>
              <ul class="media-list">
				#foreach($order in $computerorderFullUnderwayList)
                <li class="media">
                  <div class="media-body">
                    <div class="media-heading"><h5><a href="">$order.computerordertitle</a></h5><span>等待你提交预约申请</span></div>
                    <span class="tag label label-important">#if($order.computerorderordertype==2)课程预约 #else 个人预约 #end</span>                  
                  </div>
                </li>
				#end
              </ul>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!--stream-wrap end-->
  </div>
  <!---main-content end-->
</section>


