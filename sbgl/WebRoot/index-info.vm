<!-- Modal -->
<div id="modify-basic" class="modal hide fade" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" style="display: none; ">
  <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true"><i class="icon-remove"></i></button>
    <h3 id="myModalLabel">基本信息</h3>
  </div>
  <div class="modal-body">
      <form class="form-horizontal" role="form">
        <div class="form-group">
          <label for="inputGender" class="col-xs-4 control-label">性别</label>
          <div class="col-xs-6">
            <label class="radio-inline">
              <input type="radio" name="inputGender" id="gender-male" value="1" checked="">男
            </label>
            <label class="radio-inline">
              <input type="radio" name="inputGender" id="gender-female" value="2">女
            </label>
          </div>
        </div>
        <div class="form-group">
          <label for="inputBirthday" class="col-xs-4 control-label">生日</label>
          <div class="col-xs-2">
            <input type="text" class="form-control" id="inputBirthday" placeholder="1989">
          </div>
          <div class="col-xs-2">
            <select class="select2 select-white form-control" id="inputBirthMonth">
              <option value="1">1</option><option value="2">2</option>
              <option value="3">3</option><option value="4">4</option>
              <option value="5">5</option><option value="6">6</option>
              <option value="7">7</option><option value="8">8</option>
              <option value="9">9</option><option value="10">10</option>
              <option value="11">11</option><option value="12">12</option>
            </select>
          </div>
          <div class="col-xs-2">
            <select class="select2 select-white form-control" id="inputBirthDay">
              <option value="1">1</option><option value="2">2</option><option value="3">3</option><option value="4">4</option>
              <option value="5">5</option><option value="6">6</option><option value="7">7</option><option value="8">8</option>
              <option value="9">9</option><option value="10">10</option><option value="11">11</option><option value="12">12</option>
              <option value="13">13</option><option value="14">14</option><option value="15">15</option><option value="16">16</option>
              <option value="17">17</option><option value="18">18</option><option value="19">19</option><option value="20">20</option>
              <option value="21">21</option><option value="22">22</option><option value="23">23</option><option value="24">24</option>
              <option value="25">25</option><option value="26">26</option><option value="27">27</option><option value="28">28</option>
              <option value="29">29</option><option value="30">30</option><option value="31">31</option>
            </select>
          </div>
        </div>
      </form>
    </div>
  <div class="modal-footer">
    <button class="btn btn-white" data-dismiss="modal" aria-hidden="true">取 消</button>
    <button class="btn btn-primary">确 定</button>
  </div>
</div>
